    "#calculator_basic operations\n",
    "a,b,c=map(str,input().split())\n",
    "d=int(a)\n",
    "e=int(c)\n",
    "if b==\"+\":\n",
    "    result=d+e\n",
    "elif b==\"-\":\n",
    "    result=d-e\n",
    "elif b==\"*\":\n",
    "    result=d*e\n",
    "elif b==\"/\":\n",
    "    result=d/e\n",
    "print(result)"
   ]
